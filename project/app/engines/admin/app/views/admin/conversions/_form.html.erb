<div class="card">
  <div class="card-block">
    <%= simple_form_for(conversion) do |f| %>
      <%= f.input :email %>
      <div class="actions">
        <%= f.submit class: 'btn btn-primary' %>
        <%= link_to 'Cancel', conversions_path, class: 'btn btn-secondary' %>
      </div>
    <% end %>
  </div>
</div>
